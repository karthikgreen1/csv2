---
serverinventory:
{% for item in csvfile.split("\n") %}
{%   if loop.index != 1 %}
{%     set list = item.split(",") %}
  - Requested_By: '{{ list[0]|trim() }}'
    Environment: '{{ list[1]|trim() }}'
    Patching_Type: '{{ list[2]|trim() }}'
    Assignment_Group: '{{ list[3]|trim() }}'
    Assigned_To: '{{ list[4]|trim() }}'
    Change_Coordinator: '{{ list[5]|trim() }}'
    Short_Description: '{{ list[6]|trim() }}'
    Description: '{{ list[7]|trim() }}'    
      
{%   endif %}
{% endfor %}
